<launch>

  <!-- sample file, copy this to /etc/ros/electric/turtlebot.launch -->

  <param name="turtlebot_node/gyro_scale_correction" value="1.3511"/>
  <param name="turtlebot_node/odom_angular_scale_correction" value="1.01"/>
  <param name="turtlebot_node/odom_linear_scale_correction" value="1.0395"/>

  <include file="$(find rbx1_bringup)/launch/turtlebot_minimal.launch">
    <arg name="urdf_file" value="$(find xacro)/xacro.py '$(find turtlebot_description)/robots/turtlebot.urdf.xacro'" />
  </include>

</launch>
