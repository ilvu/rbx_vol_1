<launch>
  <!-- start gazebo with an empty plane -->
  <param name="/use_sim_time" value="true" />

  <node name="gazebo" pkg="gazebo" type="gazebo" args="-u $(find rbx_gazebo)/worlds/empty.world" respawn="false" output="screen"/>

  <include file="$(find turtlebot_gazebo)/launch/robot.launch"/>

  <node pkg="tf" type="static_transform_publisher" name="odom_combined_broadcaster" args="0 0 0 0 0 0 /odom_combined /odom 100" />

</launch>
